//más operaciones
const num1 = "20";
const num2 ="20.2";
const num3 = "uno";
const num4 = 20;

console.log(num1);

//Convertir de string a numero fflotantes o enteros
console.log(Number.parseInt(num1)); //Convieerte a un entero
console.log(Number.parseFloat(num2));
console.log(Number.parseInt(num3));

//Revisar si un valor es en número entero

console.log(Number.isInteger(num4));
console.log(Number.isInteger(num3));

//Convertir numero a string

console.log(num4.toString());